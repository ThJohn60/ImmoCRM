<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.TextFlow?>

<DialogPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.immocrm.gui.help.AboutController">
	<header>
		<TextFlow fx:id="headerPane" textAlignment="CENTER" styleClass="dlg-header" >
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</TextFlow>
	</header>
	<content>
		<VBox prefHeight="200.0" prefWidth="450.0">
			<children>
				<Region prefHeight="20.0" />
				<TextFlow fx:id="aboutPane" textAlignment="CENTER" styleClass="alert-text">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</TextFlow>
			</children>
		</VBox>
	</content>
</DialogPane>
