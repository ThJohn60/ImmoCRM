<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<DialogPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<header>
		<Label id="headerLabel" styleClass="alert-header" text="" />
	</header>
	<content>
		<BorderPane prefHeight="160.0" prefWidth="500.0">
			<center>
				<Label id="msg" alignment="BOTTOM_CENTER" style="-fx-font: NORMAL 12 Tahoma;" wrapText="true" textAlignment="CENTER" styleClass="alert-text" />
			</center>
			<left>
				<VBox id="graphicPane" alignment="TOP_CENTER">
					<padding>
						<Insets bottom="0.0" left="10.0" right="0.0" top="20.0" />
					</padding>
				</VBox>
			</left>
		</BorderPane>
	</content>
	<graphic>
	</graphic>
</DialogPane>

